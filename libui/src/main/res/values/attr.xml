<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LevelImageView">
        <attr name="level" format="float"></attr>
        <attr name="drawableLeft" format="reference" />
        <attr name="drawableRight" format="reference" />
    </declare-styleable>

    <declare-styleable name="FuseImageView">
        <attr name="drawableDefault" format="reference" />
        <attr name="drawableFused" format="reference" />
    </declare-styleable>

    <declare-styleable name="DragScoreView">
        <attr name="holderIconEnable" format="reference" />
        <attr name="holderIconDisable" format="reference" />
        <attr name="drag_max" format="float|integer" />
        <attr name="drag_min" format="float|integer" />
        <attr name="scaleArr" format="string" />
        <attr name="selectColor" format="reference|color" />
        <attr name="unSelectColor" format="reference|color" />
    </declare-styleable>

</resources>